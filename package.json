    "@graphql-codegen/typescript-operations": "1.18.3",
    "@graphql-codegen/typescript-react-apollo": "2.3.0",
    "@types/node-fetch": "^2.5.12",
    "@types/react": "16.9.4",
    "autoprefixer": "^10.3.2",
    "graphql-mini-transforms": "^3.1.0",
    "postcss": "^8.3.6",
    "prettier": "^2.3.2",
    "pretty-js": "^0.2.2",
    "sass": "1.32.12",
    "tailwindcss": "^2.2.7",
    "typescript": "4.3.2"
  }
